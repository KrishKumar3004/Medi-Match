<%- include('../partials/head')%>
<%- include('../partials/navbar')%>




<% for( let doctor of new_doctors) { %>
    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4 col-xxl-4">
<div class="card text-center mb-3 bg-transparent border-success" style="background-size: cover;max-width:400px; ;box-shadow: rgba(0, 0, 0, 0.19) 0px 10px 20px, rgba(0, 0, 0, 0.23) 0px 6px 6px;" data-aos="zoom-in">
    <img src="<%= doctor.image %>" class="card-img-top card-img-can-change" alt="loading...">
    <div class="card-body">
        <h5 class="card-title font-weight-bold "><%= doctor.name %></h5>
        <p class="card-text text-muted"><%= doctor.degree %>, <%= doctor.department %></p>
        <a href="/doctors/<%= doctor._id %>" class="btn btn-success">Get Appointment</a>
    </div>
</div>
</div>
 <%}%> 

<%- include('../partials/footer')%>